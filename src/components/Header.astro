---
import { getCollection } from "astro:content";

import HeaderSvelte from "../components/Svelte/HeaderSvelte.svelte";

const posts = await getCollection("info");

const Posts = posts.map((post) => {
  return {
    id: post.data.id,
    slug: post.slug,
  };
});
---

<HeaderSvelte Posts={Posts} client:load />


